{
  "manifest_version": 2,
  "name": "BoilerTVContentList",
  "description": "Show content available from BoilerTV when trying to figure out where to watch a movie or TV show",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "web_accessible_resources": [
    "icons/*.svg",
    "icons/*.png"
  ],
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}